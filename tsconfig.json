{
  "compileOnSave": false,
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "jsx": "react",
    "noImplicitAny": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "charset": "utf8",
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "importHelpers": true,
    "baseUrl": ".",
    "typeRoots": [
      "./node_modules/@types/",
      "./client/@types/"
    ],
    "paths": {
      "*": ["*"],
      "@components/*": ["client/components/*"],
      "@styles/*": ["client/styles/*"],
      "@utils/*": ["client/utils/*"],
      "@redux/*": ["client/redux/*"],
      "@logger": ["client/utils/logger"],
      "@config/*": ["config/*"]
    },
    "lib": [
      "dom",
      "es2015",
      "es2016"
    ]
  }
}